//프로그래머스 코딩테스트 입문 두 수의 차
#include <stdio.h>
#include <stdbool.h>
#include <stdlib.h>

int solution(int num1, int num2) {
    int answer = 0;

    answer = num1 - num2;

    return answer;
}

//프로그래머스 코딩테스트 입문 몫 구하기
int solution(int num1, int num2) {
    int answer = 0;
    answer = num1 / num2;
    return answer;
}

//프로그래머스 코딩테스트 입문 나머지 구하기
#include <stdio.h>
#include <stdbool.h>
#include <stdlib.h>

int solution(int num1, int num2) {
    int answer = -1;
    answer = num1 % num2;
    return answer;
}

//프로그래머스 코딩테스트 입문 두 수의 곱
int solution(int num1, int num2) {
    int answer = 0;
    answer = num1 * num2;
    return answer;
}

//프로그래머스 코딩테스트 입문 숫자 비교하기
#include <stdio.h>
#include <stdbool.h>
#include <stdlib.h>

int solution(int num1, int num2) {
    int answer = 0;
    if(num1 == num2) {
        answer = 1;
    } else {
        answer = -1;
    }
    return answer;
}

//프로그래머스 코딩테스트 입문 두 수의 합
#include <stdio.h>
#include <stdbool.h>
#include <stdlib.h>

int solution(int num1, int num2) {
    int answer = -1;
    answer = num1 + num2;
    return answer;
}

//프로그래머스 코딩테스트 입문 나이 출력
#include <stdio.h>
#include <stdbool.h>
#include <stdlib.h>

int solution(int age) {
    int answer = 0;
    answer = 2022 - age + 1;
    return answer;
}

//프로그래머스 코딩테스트 입문 각도기
#include <stdio.h>
#include <stdbool.h>
#include <stdlib.h>

int solution(int angle) {
    int answer = 0;
    if (0 < angle && angle < 90 ) {
        answer = 1;
    } else if (angle == 90) {
        answer = 2;
    } else if (90 < angle && angle < 180) {
        answer = 3;
    } else {
        answer = 4;
    }
    return answer;
}

//프로그래머스 코딩테스트 입문 짝수의 합
#include <stdio.h>
#include <stdbool.h>
#include <stdlib.h>

int solution(int n) {
    int answer = 0;
    for(int i = 0; i <= n; i++) {
        if(i%2 == 0) {
            answer += i;
        }
    }
    return answer;
}

//프로그래머스 코딩테스트 입문 양꼬치
#include <stdio.h>
#include <stdbool.h>
#include <stdlib.h>

int solution(int n, int k) {
    int answer = 0;
    answer = 12000*n+2000*(k-n/10);
    return answer;
}
